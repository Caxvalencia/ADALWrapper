{
  "name": "bc-adal-angular",
  "version": "0.0.5",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage && http-server -c-1 -o -p 9875 ./coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "build": "npm run packagr",
    "build:test": "npm test && npm run lint && npm run build",
    "build:pack": "npm run build:test && cd dist/bc-adal-angular && npm pack",
    "build:publish": "npm run build && cd dist/bc-adal-angular && npm publish",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dericop/ADALWrapper.git"
  },
  "private": false,
  "peerDependencies": {
    "adal-angular": "^1.0.17",
    "@angular/common": ">=6.1.0",
    "@angular/core": ">=6.1.0",
    "@angular/router": ">=6.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~6.2.8",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.10",
    "@types/adal-angular": "^1.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "adal-angular": "^1.0.17",
    "codelyzer": "~4.3.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "adal-angular": "^1.0.17"
  }
}
